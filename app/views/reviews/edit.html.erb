<div class="col-md-6 col-md-offset-3">

	<div class="edit-review-form">

		<h1>Edit Review</h1>

		<%= simple_form_for([@restaurant, @dish, @review]) do |form| %>
            <div class="field">
                <%= form.label :body %>
                <%= form.text_field :body %>
            </div>

            <div class="actions">
                <%= form.button :submit %>
            </div>
		<% end %>	

	</div>

</div>

<%= link_to 'Back', root_path %>